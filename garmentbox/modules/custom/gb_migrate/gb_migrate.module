<?php

/**
 * Implements hook_migrate_api().
 */
function gb_migrate_migrate_api() {
  $mirgations = array(
    // Users.
    'GbUsersMigrate',

    // Terms.
    'GbItemStatusTermsMigrate',
    'GbMaterialTypeTermsMigrate',
    'GbMeasurementUnitTermsMigrate',
    'GbSeasonStatusTermsMigrate',

    // Nodes.
    'GbCompaniesMigrate',
    'GbItemsMigrate',
    'GbItemVariantsMigrate',
    'GbMaterialsMigrate',
    'GbSeasonsMigrate',
  );

  $api['api'] = 2;
  foreach ($mirgations as $mirgation) {
    $api['migrations'][$mirgation] = array('class_name' => 'GbUsersMigrate');
  }

  return $api;
}
